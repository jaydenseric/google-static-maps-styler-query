{
  "name": "google-static-maps-styler-query",
  "version": "1.0.0",
  "description": "Converts a Google Maps styler array to a Google Static Maps styler URL query string.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "http://jaydenseric.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenseric/google-static-maps-styler-query.git"
  },
  "homepage": "https://github.com/jaydenseric/google-static-maps-styler-query#readme",
  "bugs": "https://github.com/jaydenseric/google-static-maps-styler-query/issues",
  "keywords": [
    "google",
    "static",
    "maps",
    "styler",
    "query",
    "string"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.1.8",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^3.2.2",
    "standard": "^9.0.0"
  },
  "scripts": {
    "lint": "standard",
    "prebuild": "npm run lint",
    "build": "rollup --config",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run build"
  },
  "main": "dist/google-static-maps-styler-query.js",
  "module": "dist/google-static-maps-styler-query.module.js",
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=6.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6.4,
            "browsers": [
              "> 2%"
            ]
          },
          "modules": false,
          "loose": true
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/**"
    ]
  }
}
